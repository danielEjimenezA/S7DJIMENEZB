<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="S7DJIMENEZB.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtNombre" Placeholder="Ingrese Nombre"/>
            <Entry x:Name="txtUser" Placeholder="Ingrese Usuario"/>
            <Entry x:Name="txtPassword" Placeholder="Ingrese Contrasena"/>
            <Button x:Name="btnRegistro" Text="Registro" Clicked="btnRegistro_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>